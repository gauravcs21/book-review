}

header nav {
	flex-grow: 1;
}

header nav ul {
	display: flex;
	list-style: none;
	margin: 0;
	padding: 0;
}

header nav ul li {
	margin-right: 30px;
}

header nav ul li:last-child {
	margin-right: 0;
}

header nav ul li a {
	color: #fff;
	text-decoration:none;
	transition: color 0.3s ease;
	
}
header nav ul li a .link {
	width: 10px;
	height: 10px;
	
}


header nav ul li a:hover {
	color: #3367d6;
}

header nav ul li input[type="search"] {
	background-color: #fff;
	border: none;
	border-radius: 5px;
	padding: 10px;
	font-size: 16px;
	width: 100%;
	box-sizing: border-box;
}

header nav ul li input[type="search"]::placeholder {
	color: #ccc;
}
header nav ul li .search-logo{
	margin-top: 10px;
	padding: 1px;
	border: 1px solid black;
	border-radius: 50%;
}
body {
